\NewDocumentCommand{\Mod}{s m o}{%
	\IfNoValueTF{#3}{%
		\IfBooleanTF{#1}{%
			\cat{Mod}\text{-}#2%
		}{%
			#2\text{-}\cat{Mod}%
		}%
	}%
	{%
		\IfBooleanTF{#1}{%
			#3\text{-}\cat{Mod}\text{-}#2%
		}{%
			#2\text{-}\cat{Mod}\text{-}#3%
		}%
	}
}
